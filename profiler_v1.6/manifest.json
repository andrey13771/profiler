{
	"name": "Profiler",
	"version": "1.6",
	"description": "Profiler will track your browsing habits to create a web profile.",
	"permissions": [
		"tabs",
		"history",
		"identity",
		"identity.email",
		"http://localhost:5000/*",
		"https://chrome-profiler.herokuapp.com/*"
	],
	"key": "",
	"oauth2": {
		"client_id": "567681894938-sijh9j09deq7flb99ocqabmu4ft8ppjg.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/userinfo.email"]
	},
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["http://*/*", "https://*/*"]
		}

	],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
